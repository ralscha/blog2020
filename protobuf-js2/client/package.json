{
  "name": "protobuf2",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "serve-dist": "ws --hostname localhost -d dist/app/browser -p 1234 -o --log.format stats",
    "pbts": "pbjs -t static-module --es6 -w es6 ../server/src/main/protobuf/User.proto -o src/app/protos/user.js && pbts --no-comments src/app/protos/user.js -o src/app/protos/user.d.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "20.3.6",
    "@angular/compiler": "20.3.6",
    "@angular/core": "20.3.6",
    "@angular/forms": "20.3.6",
    "@angular/platform-browser": "20.3.6",
    "@angular/router": "20.3.6",
    "protobufjs": "7.5.4",
    "rxjs": "7.8.2",
    "tslib": "2.8.1",
    "zone.js": "0.15.1"
  },
  "devDependencies": {
    "@angular/build": "20.3.6",
    "@angular/cli": "20.3.6",
    "@angular/compiler-cli": "20.3.6",
    "local-web-server": "5.4.0",
    "protobufjs-cli": "1.1.3",
    "typescript": "5.9.3"
  }
}